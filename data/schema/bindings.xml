<?xml version="1.0" encoding="UTF-8"?>
<jxb:bindings version="2.0" xmlns:jxb="http://java.sun.com/xml/ns/jaxb" xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<jxb:bindings schemaLocation="state.xsd" node="/xs:schema">
		<jxb:schemaBindings>
			<jxb:package name="edu.cwru.sepia.environment.model.persistence.generated"/>
			<jxb:nameXmlTransform>
				<jxb:elementName prefix="Xml"/>
				<jxb:typeName prefix="Xml"/>
			</jxb:nameXmlTransform>
		</jxb:schemaBindings>
		<jxb:bindings node="xs:simpleType[@name='ResourceType']">
			 <jxb:javaType name="edu.cwru.sepia.environment.model.state.ResourceType" 
			 	parseMethod="ResourceType.valueOf"/>
		</jxb:bindings>
		<jxb:bindings node="xs:simpleType[@name='ActionFeedback']">
			 <jxb:javaType name="edu.cwru.sepia.action.ActionFeedback" 
			 	parseMethod="ActionFeedback.valueOf"/>
		</jxb:bindings>
		<jxb:bindings node="xs:simpleType[@name='ResourceNodeType']">
			 <jxb:javaType name="edu.cwru.sepia.environment.model.state.ResourceNode.Type" 
			 	parseMethod="Type.valueOf"/>
		</jxb:bindings> 
		<jxb:bindings node="xs:simpleType[@name='Direction']">
			 <jxb:javaType name="edu.cwru.sepia.util.Direction" 
			 	parseMethod="Direction.valueOf"/>
		</jxb:bindings> 
		<jxb:bindings node="xs:simpleType[@name='ActionType']">
			 <jxb:javaType name="edu.cwru.sepia.action.ActionType" 
			 	parseMethod="ActionType.valueOf"/>
		</jxb:bindings> 
	</jxb:bindings>
</jxb:bindings>